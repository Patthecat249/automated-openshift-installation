DEFAULT menu.c32
PROMPT 0

MENU TITLE CentOS-Server-Install-Menu
MENU AUTOBOOT Starting CentOS in # seconds

timeout 18
ONTIMEOUT centOS

LABEL centOS
menu label Install {{ hostname }}
kernel kernels_initrd/centos77/vmlinuz
append ip=dhcp initrd=kernels_initrd/centos77/initrd.img inst.repo=nfs:nas.home.local:/volume1/nfs-iso/downloaded-iso/linux/CentOS-7-x86_64-DVD-1908.iso inst.ks=nfs:nas.home.local:/volume1/nfs-iso/kickstart-configs/registry.cfg

